{
  "env": {
    "node": true
  },
  "extends": "airbnb",
  rules: {
    "no-console": 0,
    "no-undef": 0,
    "space-before-function-paren": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-else-return": 0,
    "comma-dangle": 0,
    "arrow-body-style": 0,
    "max-len": 0,
    "react/prop-types": 0,
    "react/jsx-no-bind": 0
  }
}